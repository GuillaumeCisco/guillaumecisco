{
    "name": "test-package",
    "version": "0.0.1",
    "devDependencies": {
        "chai": "4.1.2",
        "enzyme": "3.3.0",
        "enzyme-adapter-react-16": "1.1.1",
        "enzyme-to-json": "3.3.4",
        "expect": "23.3.0",
        "istanbul": "1.1.0-alpha.1",
        "jest": "23.3.0",
        "jest-emotion-react": "0.0.2",
        "jest-glamor-react": "4.3.1",
        "mocha": "5.2.0",
        "react-test-renderer": "16.4.1",
        "redux-mock-store": "1.5.3",
        "sinon": "6.1.3",
        "sinon-chai": "3.2.0"
    },
    "scripts": {
        "test": "cd ../../ && cross-env NODE_ENV=development ./node_modules/.bin/mocha --require babel-core/register \"./src/**/*.spec.js\" && cd -",
        "jest-test": "jest",
        "coverage": "jest --coverage --no-cache --ci --runInBand",
        "cover": "cd ../../ && ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --require babel-core/register \"./src/frontend/js/**/*.spec.js\" && cd -"
    }
}
